        ];

        let currentTermIndex = -1;
        let isHidden = false;

        function shuffleArray(array) {
            const newArray = [...array];
            for (let i = newArray.length - 1; i > 0; i--) {
                const j = Math.floor(Math.random() * (i + 1));
                [newArray[i], newArray[j]] = [newArray[j], newArray[i]];
            }
            return newArray;
        }

        function nextTerm() {
            const availableIndices = terms.map((_, i) => i).filter(i => i !== currentTermIndex);
            currentTermIndex = availableIndices[Math.floor(Math.random() * availableIndices.length)];
            
            const termElement = document.getElementById('term');
            termElement.textContent = terms[currentTermIndex];
            termElement.classList.remove('hidden');
            isHidden = false;
            
            document.querySelector('.hide-btn').textContent = 'Hide Term';
        }

        function toggleTerm() {
            const termElement = document.getElementById('term');
            const button = document.querySelector('.hide-btn');
            
            isHidden = !isHidden;
            
            if (isHidden) {
                termElement.classList.add('hidden');
                button.textContent = 'Show Term';
            } else {
                termElement.classList.remove('hidden');
                button.textContent = 'Hide Term';
            }
        }

        function createEmojiKeyboard() {
            const keyboard = document.getElementById('emojiKeyboard');
            const shuffledEmojis = shuffleArray(emojis);
            
            shuffledEmojis.forEach(emoji => {
                const button = document.createElement('button');
                button.className = 'emoji-btn';
                button.textContent = emoji;
                button.onclick = () => addEmoji(emoji);
                keyboard.appendChild(button);
            });
        }

        function addEmoji(emoji) {
            const input = document.getElementById('emojiInput');
            const start = input.selectionStart;
            const end = input.selectionEnd;
            const text = input.value;
            
            input.value = text.substring(0, start) + emoji + text.substring(end);
            input.focus();
            input.selectionStart = input.selectionEnd = start + emoji.length;
        }

        // Initialize
        createEmojiKeyboard();
        nextTerm();
    </script>
</body>
</html>
